<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.effect.*?>
<?import javafx.scene.text.*?>
<?import javafx.geometry.*?>
<?import java.lang.*?>
<?import javafx.scene.control.*?>
<?import javafx.scene.layout.*?>
<?import javafx.scene.layout.BorderPane?>

<BorderPane xmlns="http://javafx.com/javafx/8" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.MehrfachEingabeController">
   <bottom>
      <ToolBar nodeOrientation="RIGHT_TO_LEFT" prefHeight="40.0" BorderPane.alignment="CENTER">
        <items>
            <Button fx:id="derCancelButton" mnemonicParsing="false" onAction="#cancel" text="Cancel" />
          <Button fx:id="derFertigButton" mnemonicParsing="false" onAction="#fertig" text="Fertig" />
        </items>
         <BorderPane.margin>
            <Insets />
         </BorderPane.margin>
      </ToolBar>
   </bottom>
   <center>
      <GridPane minHeight="500.0" BorderPane.alignment="CENTER">
        <columnConstraints>
          <ColumnConstraints hgrow="SOMETIMES" minWidth="200.0" />
          <ColumnConstraints hgrow="SOMETIMES" maxWidth="190.0" minWidth="10.0" prefWidth="50.0" />
        </columnConstraints>
        <rowConstraints>
          <RowConstraints maxHeight="50.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
          <RowConstraints minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
        </rowConstraints>
         <children>
            <Accordion fx:id="dasAkkordeon" GridPane.columnSpan="2" GridPane.rowIndex="1">
              <panes>
                <TitledPane fx:id="dasTitledPane1" text="Äußere Funktion" />
                <TitledPane fx:id="dasTitledPane2" expanded="false" text="Innere Funktion" />
              </panes>
            </Accordion>
            <Label fx:id="dasFunktionsLabel" alignment="TOP_LEFT" text="Funktion">
               <font>
                  <Font name="Times New Roman" size="26.0" />
               </font>
               <GridPane.margin>
                  <Insets bottom="5.0" left="10.0" />
               </GridPane.margin>
            </Label>
            <Button fx:id="derHinzufuegenButton" mnemonicParsing="false" onAction="#klickeHinzufuegen" text="+" GridPane.columnIndex="1" GridPane.halignment="RIGHT">
               <GridPane.margin>
                  <Insets bottom="5.0" right="10.0" />
               </GridPane.margin>
            </Button>
         </children>
         <BorderPane.margin>
            <Insets top="10.0" />
         </BorderPane.margin>
      </GridPane>
   </center>
</BorderPane>
